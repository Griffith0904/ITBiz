<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mabatis.org//DTD Mapper 3.0//EN"
                       "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="user">
	<select id="getLoginUserInfo" parameterType="kr.co.hojeon.beans.UserBean" resultType="kr.co.hojeon.beans.UserBean">
		<![CDATA[
			Select deptseq, deptname, empid, empname, email, userid, typename, dbname, jpname, jdname, joname, dbseq
			  From tb_user_list with(nolock)
			 Where userid = #{userid}
			   And userpw = #{userpw}
		]]>		
	</select>
</mapper>